@model IEnumerable<NguyenPhanHuy_2122110062.Models.Context.ProductCategory>

@if (Model != null && Model.Any())
{
    <article class="filter-group">
        <h6 class="title">
            <a href="#" class="dropdown-toggle" data-toggle="collapse" data-target="#collapse_1">	 Product type </a>
        </h6>
        <div class="filter-content collapse show" id="collapse_1" style="">
            <div class="inner">
                <ul class="list-menu">
                    @foreach (var item in Model)
                    {
                        <li><a href="@Url.Action("Index", "Product", new { type = "category", id = item.Id })">@item.Title</a></li>
                    }
                </ul>
            </div> <!-- inner.// -->
        </div>
    </article> <!-- filter-group  .// -->
}